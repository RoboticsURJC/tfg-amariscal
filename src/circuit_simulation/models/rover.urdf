<?xml version="1.0"?>
<robot name="rover">
	<link name="LinkWheelBackLeft">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="50" />
			<inertia ixx="10" ixy="10" ixz="10" iyy="10" iyz="10" izz="10" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</visual>
		<collision name="WheelBackLeft">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</collision>
	</link>

	<link name="LinkWheelBackRight">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="50" />
			<inertia ixx="10" ixy="10" ixz="10" iyy="10" iyz="10" izz="10" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</visual>
		<collision name="WheelBackRight">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</collision>
	</link>

	<link name="LinkWheelFrontLeft">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="50" />
			<inertia ixx="10" ixy="10" ixz="10" iyy="10" iyz="10" izz="10" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</visual>
		<collision name="WheelFrontLeft">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</collision>
	</link>

	<link name="LinkWheelFrontRight">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="50" />
			<inertia ixx="10" ixy="10" ixz="10" iyy="10" iyz="10" izz="10" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</visual>
		<collision name="WheelFrontRight">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.47719" length="0.23635" />
			</geometry>
		</collision>
	</link>

	<link name="MainLink">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="80.0" />
			<inertia ixx="10" ixy="10" ixz="10" iyy="10" iyz="10" izz="10" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="3.79233 2.0 0.7" />
			</geometry>
		</visual>
		<collision name="MainCube">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="3.79233 2.0 0.7" />
			</geometry>
		</collision>
	</link>

	<link name="CameraLink">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="1.0" />
			<inertia ixx="10" ixy="10" ixz="10" iyy="10" iyz="10" izz="10" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="0.2 0.2 0.2" />
			</geometry>
		</visual>
		<collision name="collision">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size="0.2 0.2 0.2" />
			</geometry>
		</collision>
	</link>

	<joint name="joint_camera" type="fixed">
		<origin xyz="1.8 0.5 0.75" rpy="0 0 0" />
		<parent link="MainLink" />
		<child link="CameraLink" />
		<axis xyz="0 0 1.0" />
	</joint>

	<joint name="joint_wheel_back_left" type="continuous">
		<origin xyz="-1.23386 1.1772 -0.46428" rpy="1.5708 0 0" />
		<parent link="MainLink" />
		<child link="LinkWheelBackLeft" />
		<axis xyz="0 0 -1.0" />
	</joint>

	<joint name="joint_wheel_back_right" type="continuous">
		<origin xyz="-1.20753 -1.15931 -0.46427" rpy="1.5708 0 0" />
		<parent link="MainLink" />
		<child link="LinkWheelBackRight" />
		<axis xyz="0 0 -1.0" />
	</joint>

	<joint name="joint_wheel_front_left" type="continuous">
		<origin xyz="1.19673 1.1772 -0.46427" rpy="1.5708 0 0" />
		<parent link="MainLink" />
		<child link="LinkWheelFrontLeft" />
		<axis xyz="0 0 -1.0" />
	</joint>

	<joint name="joint_wheel_front_right" type="continuous">
		<origin xyz="1.19673 -1.15931 -0.46427" rpy="1.5708 0 0" />
		<parent link="MainLink" />
		<child link="LinkWheelFrontRight" />
		<axis xyz="0 0 -1.0" />
	</joint>

	<gazebo>
		<plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
			<robotNamespace>/rover</robotNamespace>
		</plugin>
	</gazebo>

	<gazebo reference="CameraLink">
		<sensor name='cam_sensor' type='camera'>
			<pose>1.8 0.5 0.75 0 0 0</pose>
			<update_rate>20.000000</update_rate>
			<camera name='CameraLink'>
				<horizontal_fov>1.570000</horizontal_fov>
				<image>
					<width>320</width>
					<height>240</height>
					<format>R8G8B8</format>
				</image>
				<clip>
					<near>0.100000</near>
					<far>15.000000</far>
				</clip>
			</camera>
			<plugin name="camera_controller" filename="libgazebo_ros_camera.so">
				<alwaysOn>true</alwaysOn>
				<updateRate>0.0</updateRate>
				<cameraName>camera</cameraName>
				<imageTopicName>image_raw</imageTopicName>
				<cameraInfoTopicName>camera_info</cameraInfoTopicName>
				<frameName>camera_top_body</frameName>
				<hackBaseline>0.07</hackBaseline>
				<distortionK1>0.0</distortionK1>
				<distortionK2>0.0</distortionK2>
				<distortionK3>0.0</distortionK3>
				<distortionT1>0.0</distortionT1>
				<distortionT2>0.0</distortionT2>
			</plugin>
		</sensor>
	</gazebo>


	<transmission name="joint_wheel_front_right_trans">
		<type>transmission_interface/SimpleTransmission</type>
		<actuator name="motor1">
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
		<joint name="joint_wheel_front_right">
			<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
		</joint>
	</transmission>

	<transmission name="joint_wheel_front_left_trans">
		<type>transmission_interface/SimpleTransmission</type>
		<actuator name="motor2">
			<mechanicalReduction>1</mechanicalReduction>
			<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
		</actuator>
		<joint name="joint_wheel_front_left">
			<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
		</joint>
	</transmission>

	<transmission name="joint_wheel_back_right_trans">
		<type>transmission_interface/SimpleTransmission</type>
		<actuator name="motor3">
			<mechanicalReduction>1</mechanicalReduction>
			<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
		</actuator>
		<joint name="joint_wheel_back_right">
			<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
		</joint>
	</transmission>

	<transmission name="joint_wheel_back_left_trans">
		<type>transmission_interface/SimpleTransmission</type>
		<actuator name="motor4">
			<mechanicalReduction>1</mechanicalReduction>
			<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
		</actuator>
		<joint name="joint_wheel_back_left">
			<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
		</joint>
	</transmission>

	<gazebo reference="MainLink">
		<material>Gazebo/Red</material>
	</gazebo>

	<gazebo reference="LinkWheelFrontRight">
		<material>Gazebo/Black</material>
	</gazebo>

	<gazebo reference="LinkWheelFrontLeft">
		<material>Gazebo/Black</material>
	</gazebo>

	<gazebo reference="LinkWheelBackRight">
		<material>Gazebo/Black</material>
	</gazebo>

	<gazebo reference="LinkWheelBackLeft">
		<material>Gazebo/Black</material>
	</gazebo>

	<gazebo reference="CameraLink">
		<material>Gazebo/Green</material>
	</gazebo>


</robot>