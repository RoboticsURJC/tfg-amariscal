	<link name="onboardCameraLink">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="0.1" />
			<inertia ixx="10" ixy="10" ixz="10" iyy="10" iyz="10" izz="10" />
		</inertial>
	</link>

	<link name="mapCameraLink" />

	<joint name="onboardCameraJoint" type="fixed">
		<origin xyz="0 -3.2 0.4" rpy="0 0 -1.57" />
		<parent link="chassisLink" />
		<child link="onboardCameraLink" />
		<axis xyz="0 0 1.0" />
	</joint>

	<joint name="mapCameraJoint" type="fixed">
		<origin xyz="0 0 20" rpy="0 1.57 -1.57" />
		<parent link="chassisLink" />
		<child link="mapCameraLink" />
		<axis xyz="0 0 1.0" />
	</joint>


	<joint name="frontLeftWheelJoint" type="continuous">
		<origin xyz="2.02598 -1.53082 -0.05739" rpy="1.5708 0 -1.5708" />
		<parent link="chassisLink" />
		<child link="frontLeftWheelLink" />
		<axis xyz="0 0 1.0" />
	</joint>

	<joint name="frontRightWheelJoint" type="continuous">
		<origin xyz="-2.01372 -1.51765 -0.05739" rpy="1.5708 0 1.5708" />
		<parent link="chassisLink" />
		<child link="frontRightWheelLink" />
		<axis xyz="0 0 1.0" />
	</joint>

	<joint name="rearRightWheelJoint" type="continuous">
		<origin xyz="-2.04563 0.93075 -0.05739" rpy="1.5708 0 1.5708" />
